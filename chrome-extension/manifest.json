{
    "manifest_version": 3,
    "name": "RoutineRadar",
    "description": "The time spent on the website",
    "version": "1.0",
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "https://*/*", 
        "http://*/*" ]
}
{
    "manifest_version": 3,
    "name": "RoutineRadar",
    "description": "Track time for different tabs",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "tabs", "scripting", "activeTab"],
    "host_permissions": [
        "https://*/*", 
        "http://*/*" 
    ]
}
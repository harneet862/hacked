{
    "manifest_version": 3,
    "name": "RoutineRadar",
    "description": "The time spent on the website",
    "version": "1.0",
    "permissions": [
        "storage",
        "tabs",
        "activeTab",
        "scripting"
    ],
    "icons": {
        "16": "icons/image2.png",
        "48": "icons/image2.png",
        "128": "icons/image2.png"
      },
    "background": {
        "service_worker": "background.js"
    },

    "action": {
            "default_popup": "popup.html",
            "default_icon": {
              
            }
        },
    "host_permissions": [
            "https://*/*", 
            "http://*/*" ]
}
